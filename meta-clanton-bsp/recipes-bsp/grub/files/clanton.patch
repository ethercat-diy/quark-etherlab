+      grub_printf("%s: couldn't find device. Skipping..\n", __func__);
+      return;
+    }
+
+  if (grub_quark_debug)
+    grub_printf("%s: MMIO addr @ 0x%x\n", __func__, mmio);
+
+  offs = grub_sprintf(p, "0x%x", mmio);
+  grub_sprintf(p + offs, "%s", p + sizeof(QUARK_UART_MMIO_TOKEN) - 1);
